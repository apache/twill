                        </configuration>
                    </plugin>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-remote-resources-plugin</artifactId>
                        <configuration>
                            <!-- Disable this plugin so that it won't generate a DEPENDENCIES file in the source tar -->
